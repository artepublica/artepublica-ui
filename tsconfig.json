{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets": [
        "./assets/*"
      ],
      "@app": [
        "./src/app"
      ],
      "@components": [
        "./src/components"
      ],
      "@domain": [
        "./src/domain"
      ],
      "@pages": [
        "./src/pages"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  },
  "inlude": [
    "src/**/*"
  ]
}