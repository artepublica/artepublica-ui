{
  "name": "arte-publica-permanente",
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky install",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "build:android": "eas build --platform android",
    "build:android:dev": "eas build --profile development --platform android",
    "build:android:test": "eas build --profile preview --platform android",
    "ios": "expo run:ios",
    "build:ios:dev": "eas build --profile development --platform ios",
    "build:ios:test": "eas build --profile preview --platform ios",
    "updade": "eas update --branch preview --message 'Updating the app'",
    "web": "expo start --web",
    "build:web": "expo export:web",
    "lint": "eslint src && tsc --noemit",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:ci": "jest --config=jest.ci.config.js",
    "test:coverage": "jest --config=jest.ci.config.js --collectCoverage"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "prepush-if-changed": {
    "src/**/*.{ts,tsx}": [
      "npm run test:coverage"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/webpack-config": "^18.0.1",
    "@formidable-webview/webshell": "^2.6.0",
    "@googlemaps/react-wrapper": "^1.1.35",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/material-top-tabs": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "dotenv": "^16.0.3",
    "expo": "~48.0.11",
    "expo-application": "~5.1.1",
    "expo-auth-session": "~4.0.3",
    "expo-constants": "~14.2.1",
    "expo-linking": "~4.0.1",
    "expo-random": "~13.1.1",
    "expo-secure-store": "^12.1.1",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "expo-web-browser": "~12.1.1",
    "highcharts": "^10.3.3",
    "highcharts-react-official": "^3.2.0",
    "html2canvas": "^1.4.1",
    "jest": "^29.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.6",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-easy-grid": "^0.2.2",
    "react-native-error-boundary": "^1.2.3",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-map-clustering": "^3.4.2",
    "react-native-maps": "1.3.2",
    "react-native-pager-view": "6.1.2",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-table-component": "^1.2.2",
    "react-native-web": "~0.18.12",
    "react-native-webview": "11.26.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/google.maps": "^3.52.3",
    "@types/jest": "^29.5.0",
    "@types/jwt-simple": "^0.5.33",
    "@types/react": "~18.0.28",
    "@types/react-native": "^0.71.4",
    "@types/react-native-table-component": "^1.2.4",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-universe": "^11.2.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "graphql-request": "^5.2.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-expo": "^48.0.2",
    "jest-html-reporters": "^3.1.4",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.2.0",
    "madge": "^6.0.0",
    "prepush-if-changed": "^1.0.8",
    "prettier": "^2.8.6",
    "prettier-plugin-multiline-arrays": "^1.1.3",
    "react-test-renderer": "^18.2.0"
  },
  "private": true
}
