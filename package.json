{
  "name": "inventario-arte-publica-rj",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "prepare": "husky install",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export:web",
    "lint": "eslint src && tsc --noemit && madge --circular .",
    "lint:fix": "eslint --fix src && tsc --noemit && madge --circular .",
    "test": "jest",
    "test:coverage": "jest --config=jest.config.js --collectCoverage"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "prepush-if-changed": {
    "src/**/*.{ts,tsx}": [
      "npm run test:coverage"
    ]
  },
  "dependencies": {
    "@expo/webpack-config": "^0.17.0",
    "expo": "~46.0.2",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.3",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-maps": "0.31.1",
    "react-native-reanimated": "^2.9.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-native": "^11.0.0",
    "@types/jest": "^28.1.6",
    "@types/react": "~18.0.15",
    "@types/react-native": "~0.69.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-universe": "^11.1.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-expo": "^46.0.1",
    "jest-html-reporters": "^3.0.10",
    "lint-staged": "^13.0.3",
    "madge": "^5.0.1",
    "prepush-if-changed": "^1.0.8",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.7.4"
  },
  "private": true
}