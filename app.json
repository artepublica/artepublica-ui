      }
    },
    "android": {
      "versionCode": "0.0.1",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.theobassan.inventarioartepublicarj",
      "config": {
        "googleMaps": {
          "apiKey": "<android maps key>"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
